maxprime22_spec_why: THEORY
BEGIN

  importing why@why
  IMPORTING caduceus_why


  %% DO NOT EDIT BELOW THIS LINE

  global: TYPE+;

  isDivisor(m:int, n:int) : bool = (mod(n,m)) = 0

  isPrime(n:int) : bool =
    (n >= 1 AND
    (FORALL (i: int): (i >= 2 AND i < n IMPLIES NOT isDivisor(i, n))))

END maxprime22_spec_why

